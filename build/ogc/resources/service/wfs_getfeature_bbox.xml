<!-- Example of a GetFeature XML POST request containing a BoundingBox condition -->
<wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs"
                xmlns:gml="http://www.opengis.net/gml"
                xmlns:ogc="http://www.opengis.net/ogc"
                service="WFS"
                version="1.1.0">
  <wfs:Query typeName="cities">
    <ogc:Filter>
      <ogc:BBOX>
        <ogc:PropertyName xmlns:ns0="city_125/gml/3.1">ns0:geometry</ogc:PropertyName>
        <gml:Envelope srsName="urn:ogc:def:crs:OGC:1.3:CRS84">
          <gml:lowerCorner>-78.943849 35.6698</gml:lowerCorner>
          <gml:upperCorner>-68.943849 45.6698</gml:upperCorner>
        </gml:Envelope>
      </ogc:BBOX>
    </ogc:Filter>
  </wfs:Query>
</wfs:GetFeature>