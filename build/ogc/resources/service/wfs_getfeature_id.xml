<!-- Example of a GetFeature XML POST request containing an object Id filter condition -->
<wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs"
                xmlns:ogc="http://www.opengis.net/ogc"
                xmlns:gml="http://www.opengis.net/gml"
                service="WFS"
                version="1.1.0">
<wfs:Query typeName="cities">
  <ogc:Filter>
    <ogc:GmlObjectId gml:id="SHP_77"/>
  </ogc:Filter>
</wfs:Query>
</wfs:GetFeature>